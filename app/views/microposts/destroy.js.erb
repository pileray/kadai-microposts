$('#flash_messages').html("<%=j render 'layouts/flash_messages' %>");
$('#feed-<%= current_user.id %>').html("<%=j render 'microposts/microposts', microposts: current_user.feed_microposts.order(id: :desc).page(params[:page]) %>");
$('#usertabs-<%= @user.id %>').html("<%=j render 'users/usertabs', user: @user %>");
$('#liketab-<%= @user.id %>').html("<%=j render 'microposts/microposts', microposts: @user.added_to_favorites.page(params[:page]).per(25) %>");
$('#usermicroposts-<%= @user.id %>').html("<%=j render 'microposts/microposts', microposts: @user.microposts.page(params[:page]).per(25) %>");